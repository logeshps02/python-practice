class Car:
    def __init__(self, make, model, year):
        self.make = make
        self.model = model
        self.year = year

    def display_info(self):
        return f"{self.year} {self.make} {self.model}"

car1 = Car("Toyota", "Corolla", 2020)
print(car1.make)  #without encapsulation
car1.make="logesh"
print(car1.display_info()) #encapsulation is used
